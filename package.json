{
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rm -rf node_modules build && mkdir build",
    "build-babel": "babel ./ -d ./build --ignore './node_modules,./build'",
    "build": "npm run clean &&  npm install && npm run build-babel",
    "production_v1": "npm run build && node ./build/src/server.js",
    "dev": "NODE_ENV=development nodemon --exec ./node_modules/.bin/babel-node server.js",
    "rm:all": "rm -rf node_modules .next out dist build",
    "production": "NODE_ENV=production nodemon --exec ./node_modules/.bin/babel-node server.js",
    "start": "npm run build && node --max-old-space-size=8192 ./build/server.js"
  },
